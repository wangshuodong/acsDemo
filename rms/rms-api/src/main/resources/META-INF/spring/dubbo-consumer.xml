<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:consumer check="false" timeout="10000"/>

    <dubbo:reference interface="com.cmiot.acs.facade.OperationCpeFacade" id="operationCpeFacade"/>
    <dubbo:reference interface="com.cmiot.ams.service.AreaService" id="amsAreaService"/>
    <dubbo:reference interface="com.cmiot.ams.service.AdminService" id="adminService"/>
    <dubbo:reference interface="com.cmiot.fileserver.service.RmsBackupService" id="rmsBackupService"/>
    <dubbo:reference interface="com.tydic.inter.app.service.GatewayHandleService" id="gatewayHandleService">
    	<dubbo:method name="factoryNotify" async="true" return="false"/>
    </dubbo:reference>
    <dubbo:reference interface="com.cmiot.ams.service.AppUserService" id="appUserService"/>
    <dubbo:reference interface="com.tydic.service.PluginDeviceService" id="pluginDeviceService"/>
    <dubbo:reference interface="com.cmiot.hoa.facade.hangzhou.HangzhouFacade" id="hangzhouFacade" url="${hoa.hangzhouFacade.address}" />
</beans>
