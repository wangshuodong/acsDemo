<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cmiot.rms.dao.mapper.LogBackupRecordMapper" >
  <resultMap id="BaseResultMap" type="com.cmiot.rms.dao.model.LoggingRecord" >
    <id column="recordid" property="recordId" jdbcType="VARCHAR" />
    <result column="backuptime" property="backupTime" jdbcType="VARCHAR" />
    <result column="logtype" property="logType" jdbcType="VARCHAR" />
    <result column="status" property="status" jdbcType="VARCHAR" />
    <result column="filepath" property="filePath" jdbcType="VARCHAR" />
  </resultMap>
  
  <insert id="insert" parameterType="com.cmiot.rms.dao.model.LoggingRecord" >
    insert into logging_bak_record(recordid,backuptime,logtype,status,filepath)
    values (#{recordId,jdbcType=VARCHAR},#{backupTime,jdbcType=VARCHAR}, #{logType,jdbcType=VARCHAR}, #{status,jdbcType=VARCHAR},#{filePath,jdbcType=VARCHAR})
  </insert>
  
  <update id="update" parameterType="com.cmiot.rms.dao.model.LoggingRecord">
  	update logging_bak_record
  	<set>
  		<if test="status != null">
  			status = #{status,jdbcType=VARCHAR}
  		</if>
  	</set>
  	where recordid = #{recordId,jdbcType=VARCHAR}
  </update>
  <select id="queryList" resultMap="BaseResultMap" parameterType="com.cmiot.rms.dao.model.LoggingRecord">
  	select recordid,backuptime,logtype,status,filepath from logging_bak_record order by backuptime desc
  </select>
</mapper>